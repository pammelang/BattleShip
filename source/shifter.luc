module shifter8 (
  input i[5] ,
  input j[7],
  input alufn[6],
  output shift_i[5],
  output shift_j[7]
  )
  {
  
  mojo_top a_mu;
  
  always {
  case(alufn[3:0]){
      4b0001: shift_i = i >> 1; //right
      
      4b0010: shift_j = j >> 1; //up
        
      4b0100: shift_i = i << 1; //left
        
      4b1000: shift_j = j << 1; //down
      
      default: shift_i = i;
               shift_j = j;
    
    }
  }
}